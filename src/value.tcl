set i 100                          ;# 整数(文字列との区別はない)
set f 1.0                          ;# 浮動小数点数(文字列との区別はない)
set str string                     ;# 文字列
array set arr {0 10 1 20 2 30}     ;# 連想配列(配列との区別はない)
set lis [list 10 20 30]            ;# リスト
puts $i
puts $f
puts $str
puts $arr(1)
puts $lis

